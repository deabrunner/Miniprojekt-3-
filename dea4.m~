clear all
A = 5;
k = 3;
N = 100;

ds=1/N;
s=(0:N)*ds;
t=0;

% (A/(k*pi))*((sin(k*pi*sv + 2*pi*t)) - sin(2*pi*t));

    xvector=(ds/2)*cos((A/(k*pi))*((sin(k*pi*s + 2*pi*t)) - sin(2*pi*t))+(A/(k*pi))*((sin(k*pi*s + 2*pi*t)) - sin(2*pi*t)));
    yvector=(ds/2)*sin((A/(k*pi))*((sin(k*pi*s + 2*pi*t)) - sin(2*pi*t))+(A/(k*pi))*((sin(k*pi*s + 2*pi*t)) - sin(2*pi*t)));
    xvector=c

axis([-0.05 1.0 0.05 0.15]);
axis('image')
plot(xvector,yvector)